<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.QuestionnaireAnswer">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">QuestionnaireAnswer_id_seq</param></generator></id>
<property name="question" type="java.lang.Integer"><column name="question"/></property>
<property name="value" type="java.lang.String"><column name="value"/></property>
<many-to-one name="m_QuestionnaireQuestion" class="com.aladdin.xsd.QuestionnaireQuestion" insert="false" update="false"><column name="question" not-null="false" /></many-to-one>
</class>
</hibernate-mapping>

