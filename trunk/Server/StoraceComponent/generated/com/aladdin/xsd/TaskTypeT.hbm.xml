<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.TaskTypeT">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">TaskTypeT_id_seq</param></generator></id>
<property name="Code" type="java.lang.Integer"><column name="Code"/></property>
<property name="Description" type="java.lang.String"><column name="Description"/></property>
<set name="Tasks" inverse="true" cascade="none"><key><column name="TaskType" not-null="true" /></key><one-to-many class="com.aladdin.xsd.Task"/></set>
</class>
</hibernate-mapping>

