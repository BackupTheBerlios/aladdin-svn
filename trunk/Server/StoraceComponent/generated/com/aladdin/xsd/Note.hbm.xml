<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.Note">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Note_id_seq</param></generator></id>
<property name="DateTimeOfNote" type="java.sql.Timestamp"><column name="DateTimeOfNote"/></property>
<property name="text" type="java.lang.String"><column name="text"/></property>
<property name="UserType" type="java.lang.String"><column name="UserType"/></property>
<property name="UserId" type="java.lang.String"><column name="UserId"/></property>
</class>
</hibernate-mapping>

