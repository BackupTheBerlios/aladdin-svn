<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.Task">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Task_id_seq</param></generator></id>
<property name="TaskType" type="java.lang.Integer"><column name="TaskType"/></property>
<property name="DateTimeAssigned" type="java.sql.Timestamp"><column name="DateTimeAssigned"/></property>
<property name="DateTimeFulfilled" type="java.sql.Timestamp"><column name="DateTimeFulfilled"/></property>
<property name="TaskStatus" type="java.lang.Integer"><column name="TaskStatus"/></property>
<property name="url" type="java.lang.String"><column name="url"/></property>
<property name="questionnaire" type="java.lang.Integer"><column name="questionnaire"/></property>
<property name="UserType" type="java.lang.String"><column name="UserType"/></property>
<property name="UserId" type="java.lang.Integer"><column name="UserId"/></property>
<many-to-one name="m_TaskTypeT" class="com.aladdin.xsd.TaskTypeT" insert="false" update="false"><column name="TaskType" not-null="false" /></many-to-one>
<many-to-one name="m_TaskStatusT" class="com.aladdin.xsd.TaskStatusT" insert="false" update="false"><column name="TaskStatus" not-null="false" /></many-to-one>
<many-to-one name="m_Questionnaire" class="com.aladdin.xsd.Questionnaire" insert="false" update="false"><column name="questionnaire" not-null="false" /></many-to-one>
</class>
</hibernate-mapping>

