<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="QuestionnaireQuestion">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">QuestionnaireQuestion_id_seq</param></generator></id>
<property name="type" type="java.lang.String"><column name="type"/></property>
<property name="isPrimary" type="boolean"><column name="isPrimary"/></property>
<property name="subquetsions" type="boolean"><column name="subquetsions"/></property>
<property name="parentid" type="int"><column name="parentid"/></property>
<property name="title" type="java.lang.String"><column name="title"/></property>
<property name="quest" type="int"><column name="quest"/></property>
<many-to-one name="m_QuestionnaireQuestion" class="QuestionnaireQuestion" ><column name="parentid" not-null="false" /></many-to-one>
<many-to-one name="m_Questionnaire" class="Questionnaire" ><column name="quest" not-null="false" /></many-to-one>
<set name="QuestionnaireQuestionAnswers" inverse="true" cascade="none"><key><column name="question" not-null="true" /></key><one-to-many class="QuestionnaireQuestionAnswer"/></set>
<set name="QuestionnaireAnswers" inverse="true" cascade="none"><key><column name="question" not-null="true" /></key><one-to-many class="QuestionnaireAnswer"/></set>
</class>
</hibernate-mapping>

