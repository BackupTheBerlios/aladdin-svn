<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="typeT">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">typeT_id_seq</param></generator></id>
<property name="Code" type="int"><column name="Code"/></property>
<property name="Description" type="java.lang.String"><column name="Description"/></property>
<set name="Users" inverse="true" cascade="none"><key><column name="type" not-null="true" /></key><one-to-many class="User"/></set>
</class>
</hibernate-mapping>

