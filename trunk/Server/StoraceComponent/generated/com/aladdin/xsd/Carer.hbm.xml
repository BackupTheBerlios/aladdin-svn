<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.Carer">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Carer_id_seq</param></generator></id>
<property name="persondata" type="java.lang.Integer"><column name="persondata"/></property>
<property name="sd" type="java.lang.Integer"><column name="sd"/></property>
<many-to-one name="m_PersonDatapersondata" class="com.aladdin.xsd.PersonData" insert="false" update="false"><column name="persondata" not-null="false" /></many-to-one>
<many-to-one name="m_SocioDemographicDatasd" class="com.aladdin.xsd.SocioDemographicData" insert="false" update="false"><column name="sd" not-null="false" /></many-to-one>
<set name="Notes" inverse="true" cascade="none"><key><column name="carer" not-null="true" /></key><one-to-many class="com.aladdin.xsd.Note"/></set>
<set name="CarerAssessments1" inverse="true" cascade="none"><key><column name="carer" not-null="true" /></key><one-to-many class="com.aladdin.xsd.CarerAssessment"/></set>
<set name="PatientCarers11" inverse="true" cascade="none"><key><column name="carer" not-null="true" /></key><one-to-many class="com.aladdin.xsd.PatientCarer"/></set>
</class>
</hibernate-mapping>

