<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="Clinician">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Clinician_id_seq</param></generator></id>
<property name="persondata" type="int"><column name="persondata"/></property>
<many-to-one name="m_PersonData" class="PersonData" ><column name="persondata" not-null="false" /></many-to-one>
<set name="Patients" inverse="true" cascade="none"><key><column name="clinician" not-null="true" /></key><one-to-many class="Patient"/></set>
</class>
</hibernate-mapping>

