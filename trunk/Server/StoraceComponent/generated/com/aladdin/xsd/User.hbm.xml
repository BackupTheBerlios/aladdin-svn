<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="User">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">User_id_seq</param></generator></id>
<property name="login" type="java.lang.String"><column name="login"/></property>
<property name="password" type="java.lang.String"><column name="password"/></property>
<property name="type" type="int"><column name="type"/></property>
<many-to-one name="m_typeT" class="typeT" ><column name="type" not-null="false" /></many-to-one>
<set name="Rights" inverse="true" cascade="none"><key><column name="usr" not-null="true" /></key><one-to-many class="Right"/></set>
</class>
</hibernate-mapping>

