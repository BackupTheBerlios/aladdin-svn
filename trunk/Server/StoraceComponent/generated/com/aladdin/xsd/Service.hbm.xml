<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.Service">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Service_id_seq</param></generator></id>
<property name="name" type="java.lang.String"><column name="name"/></property>
<set name="Rightss" inverse="true" cascade="none"><key><column name="srv" not-null="true" /></key><one-to-many class="com.aladdin.xsd.Rights"/></set>
</class>
</hibernate-mapping>

