<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.xsd.CarerAssessment">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">CarerAssessment_id_seq</param></generator></id>
<property name="carer" type="java.lang.Integer"><column name="carer"/></property>
<property name="DateOfAssessment" type="java.sql.Timestamp"><column name="DateOfAssessment"/></property>
<property name="RelevantHealthProblem" type="java.lang.String"><column name="RelevantHealthProblem"/></property>
<property name="SeverityOfBurden" type="java.lang.Integer"><column name="SeverityOfBurden"/></property>
<property name="EmotionalOrMentalDisorder" type="java.lang.String"><column name="EmotionalOrMentalDisorder"/></property>
<property name="PsychoactiveDrugs" type="java.lang.String"><column name="PsychoactiveDrugs"/></property>
<property name="QualityOfLife" type="java.lang.Integer"><column name="QualityOfLife"/></property>
<many-to-one name="m_Carercarer" class="com.aladdin.xsd.Carer" insert="false" update="false"><column name="carer" not-null="false" /></many-to-one>
</class>
</hibernate-mapping>

