<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="Right">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Right_id_seq</param></generator></id>
<property name="srv" type="int"><column name="srv"/></property>
<property name="usr" type="int"><column name="usr"/></property>
<many-to-one name="m_Service" class="Service" ><column name="srv" not-null="false" /></many-to-one>
<many-to-one name="m_User" class="User" ><column name="usr" not-null="false" /></many-to-one>
</class>
</hibernate-mapping>

