<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="Service">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Service_id_seq</param></generator></id>
<property name="name" type="String"><column name="name"/></property>
<set name="Rights" inverse="true" cascade="none"><key><column name="srv" not-null="true" /></key><one-to-many class="Right"/></set>
</class>
</hibernate-mapping>

