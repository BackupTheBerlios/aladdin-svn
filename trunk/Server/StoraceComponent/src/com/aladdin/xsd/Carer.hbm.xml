<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="Carer">
<id name="id" type="int" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Carer_id_seq</param></generator></id>
<property name="persondata" type="int"><column name="persondata"/></property>
<property name="sd" type="int"><column name="sd"/></property>
<many-to-one name="m_PersonData" class="PersonData" ><column name="persondata" not-null="false" /></many-to-one>
<many-to-one name="m_SocioDemographicData" class="SocioDemographicData" ><column name="sd" not-null="false" /></many-to-one>
<set name="CarerAssessments" inverse="true" cascade="none"><key><column name="carer" not-null="true" /></key><one-to-many class="CarerAssessment"/></set>
<set name="PatientCarers" inverse="true" cascade="none"><key><column name="carer" not-null="true" /></key><one-to-many class="PatientCarer"/></set>
</class>
</hibernate-mapping>

