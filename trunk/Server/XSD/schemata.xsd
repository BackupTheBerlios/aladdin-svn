	<complexType name="OperationResult">
        <annotation>
        	<documentation>OperationResult contains the code of an operation result, the status of the result (passed or failed) and the operation result description</documentation></annotation>
        <sequence>
			<element name="Code" type="string" maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation>The code of the parameter</documentation></annotation></element>
			<element name="Status" type="unsignedByte" maxOccurs="1" minOccurs="1">
				<annotation>
					<documentation>The status of the parameter</documentation></annotation></element>
			<element name="Description" type="string" maxOccurs="1" minOccurs="0">
				<annotation>
					<documentation>The description of the parameter</documentation></annotation></element>
		</sequence>
	</complexType>

    <complexType name="MediaContent">
        <annotation>
        	<documentation>Media Content</documentation>
