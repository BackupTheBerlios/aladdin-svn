<mapping resource="com/aladdin/sc/db/Clinician.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/Communication.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/ExternalService.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/Identifier.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/Measurement.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/PatientAssessment.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/PatientCarer.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/Patient.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/PersonData.hbm.xml"/>
<mapping resource="com/aladdin/sc/db/QuestionnaireAnswer.hbm.xml"/>
