<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.sc.db.Clinician">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">Clinician_id_seq</param></generator></id>
<property name="persondata" type="java.lang.Integer"><column name="persondata"/></property>
<many-to-one name="m_PersonDatapersondata" class="com.aladdin.sc.db.PersonData" insert="false" update="false"><column name="persondata" not-null="false" /></many-to-one>
<set name="Patients" inverse="true" cascade="none"><key><column name="clinician" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Patient"/></set>
</class>
</hibernate-mapping>

