<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.sc.db.dict">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">dict_id_seq</param></generator></id>
<property name="code" type="java.lang.Integer"><column name="code"/></property>
<property name="description" type="java.lang.String"><column name="description"/></property>
<property name="type" type="java.lang.String"><column name="type"/></property>
<property name="lang" type="java.lang.String"><column name="lang"/></property>
</class>
</hibernate-mapping>

