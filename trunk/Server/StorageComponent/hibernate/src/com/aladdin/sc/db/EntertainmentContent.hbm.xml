<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.aladdin.sc.db"><class name="EntertainmentContent">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">EntertainmentContent_id_seq</param></generator></id>
<property name="title" type="string"><column name="title"/></property>
<property name="url" type="string"><column name="url"/></property>
<property name="type" type="string"><column name="type"/></property>
<property name="category" type="string"><column name="category"/></property>
<property name="text" type="string"><column name="text"/></property>
</class>
</hibernate-mapping>

