<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.aladdin.sc.db"><class name="ExternalService">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">ExternalService_id_seq</param></generator></id>
<property name="Address" type="String"><column name="Address"/></property>
<property name="Description" type="String"><column name="Description"/></property>
</class>
</hibernate-mapping>

