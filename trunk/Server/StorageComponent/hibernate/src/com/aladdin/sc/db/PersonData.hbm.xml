<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping><class name="com.aladdin.sc.db.PersonData">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">PersonData_id_seq</param></generator></id>
<property name="Surname" type="java.lang.String"><column name="Surname"/></property>
<property name="Name" type="java.lang.String"><column name="Name"/></property>
<set name="Identifiers" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Identifier"/></set>
<set name="Addresss1" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Address"/></set>
<set name="Communications11" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Communication"/></set>
<set name="Clinicians111" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Clinician"/></set>
<set name="Carers1111" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Carer"/></set>
<set name="Patients11111" inverse="true" cascade="none"><key><column name="persondata" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Patient"/></set>
<set name="Administrators111111" inverse="true" cascade="none"><key><column name="personData" not-null="true" /></key><one-to-many class="com.aladdin.sc.db.Administrator"/></set>
</class>
</hibernate-mapping>

