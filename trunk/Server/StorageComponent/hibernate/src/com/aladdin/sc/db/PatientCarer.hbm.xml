<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
	PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.aladdin.sc.db"><class name="PatientCarer">
<id name="id" type="java.lang.Integer" column="id" unsaved-value="null"><generator class="sequence"><param name="sequence">PatientCarer_id_seq</param></generator></id>
<property name="patient" type="java.lang.Integer"><column name="patient"/></property>
<property name="carer" type="java.lang.Integer"><column name="carer"/></property>
<property name="isprimary" type="java.lang.Boolean"><column name="isprimary"/></property>
<many-to-one insert="false" update="false" name="m_Patient" class="Patient" ><column name="patient" not-null="false" /></many-to-one>
<many-to-one insert="false" update="false" name="m_Carer" class="Carer" ><column name="carer" not-null="false" /></many-to-one>
</class>
</hibernate-mapping>

